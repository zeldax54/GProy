@model IEnumerable<GProyOficial.Models.Entity>

@{
    ViewBag.Title = "Entidades";
    var db = new GProyOficial.Models.GProyEntities();
    List<GProyOficial.Models.Menu> t = db.Menu.SqlQuery("Select * from Menu where fatherid='3'").ToList();
    @section clients{

        @foreach (var item in t)
        {
            <li>@Html.ActionLink(@item.name, "Index", @item.url, null, new { @class = "nav navbar-brand" })</li>
        }

    }
}

<h3>Entidades</h3>

<p>
  
    @Html.ActionLink("Crear", "Create", null, new { @class = "btn btn-primary btn-sm" })
</p>
<table class="table">
    <tr>
        <th>
            <!--@Html.DisplayNameFor(model => model.name)-->Nombre
        </th>
        <th>
            <!--@Html.DisplayNameFor(model => model.reupCode)-->Codigo Reup
        </th>
        <th>
            <!--@Html.DisplayNameFor(model => model.identifAbrev)-->Identificador
        </th>
        <th>
            <!--@Html.DisplayNameFor(model => model.Organism.name)-->Organismo
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.reupCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.identifAbrev)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Organism.name)
        </td>
        <td>
           <!-- @Html.ActionLink("Editar", "Edit", new { id=item.entityId })-->
            <a href="@Url.Action("Edit", "Entities", new { id = item.entityId })" id="prueba"><img src="@Url.Content("~/Content/images/edit.png")" title="Editar" /> </a> |
            <!--@Html.ActionLink("Detalles", "Details", new { id=item.entityId })-->
            <a href="@Url.Action("Details", "Entities", new { id = item.entityId })" id="prueba"><img src="@Url.Content("~/Content/images/detail.png")" title="Detalles" /> </a> |
           <!-- @Html.ActionLink("Eliminar", "Delete", new { id=item.entityId })-->
            <a href="@Url.Action("Delete", "Entities", new { id = item.entityId })" id="prueba"><img src="@Url.Content("~/Content/images/trash.png")" title="Eliminar" /> </a>
        </td>
    </tr>
}

</table>
