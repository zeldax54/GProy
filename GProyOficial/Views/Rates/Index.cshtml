             <!--@Html.ActionLink("Detalles", "Details", new { id=item.rateId })-->
            <a href="@Url.Action("Details", "Rates", new { id = item.rateId })" id="prueba"><img src="@Url.Content("~/Content/images/detail.png")" title="Detalles" /> </a> |
             <!--@Html.ActionLink("Eliminar", "Delete", new { id=item.rateId })-->
