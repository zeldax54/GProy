@model IEnumerable<GProyOficial.Models.Rate>

@{
    ViewBag.Title = "Tarifas";
    
}

<h3>Tarifas</h3>

<p>
        @Html.ActionLink("Crear", "Create", null, new { @class = "btn btn-primary btn-sm" })

</p>
<table class="table">
    <tr>
        <th>
            <!--@Html.DisplayNameFor(model => model.rate1)-->Tarifas
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.rate1)
        </td>
        <td>
             <!-- @Html.ActionLink("Editar", "Edit", new { id=item.rateId })-->
