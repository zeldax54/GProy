@model GProyOficial.Models.CurrencyType

@{
    ViewBag.Title = "Nuevo Tipo de Moneda";
}

<h3>Nuevo Tipo de Moneda</h3>

@section clients{
    <li>
        @Html.ActionLink("Clientes", "Index", "Clients", null, new { @class = "format-link" })
    </li>
    <li>
        @Html.ActionLink("Bancos", "Index", "Banks", null, new { @class = "format-link" })
    </li>
    <li>
        @Html.ActionLink("Tipo de Modena", "Index", "CurrencyTypes", null, new { @class = "format-link" })
    </li>
    <li>
        @Html.ActionLink("Cuentas Bancarias", "Index", "AccountBanks", null, new { @class = "format-link" })
    </li>
    <li>
        @Html.ActionLink("Contratos", "Index", "Contracts", null, new { @class = "format-link" })
    </li>
    <li>
        @Html.ActionLink("Contratos Suplementarios", "Index", "Supplements", null, new { @class = "format-link" })
    </li>
    <li>
        @Html.ActionLink("Cronograma", "Index", "Schedules", null, new { @class = "format-link" })
    </li>
}

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
       
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            @Html.LabelFor(model => model.type, "Tipo", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.type, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.type, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
               
            </div>
        </div>
        @Html.ActionLink("Cancelar", "Index", null, new { @class = "btn btn-danger btn-sm" }) <input type="submit" value="Aceptar" class="btn btn-default" />
    </div>
}


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
