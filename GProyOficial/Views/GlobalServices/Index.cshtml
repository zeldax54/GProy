@model IEnumerable<GProyOficial.Models.GlobalService>

@{
    ViewBag.Title = "Servicios Globales";
    
}

<h3>Servicios Globales</h3>

<p>
    @Html.ActionLink("Crear", "Create", null, new { @class = "btn btn-primary btn-sm" })
</p>
<table class="table">
    <tr>
        <th>
            <!--@Html.DisplayNameFor(model => model.name)-->Nombre
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
           <!-- @Html.ActionLink("Editar", "Edit", new { id=item.globalServiceId })-->
            <a href="@Url.Action("Edit", "GlobalServices", new { id = item.globalServiceId})" id="prueba"><img src="@Url.Content("~/Content/images/edit.png")" title="Editar" /> </a> |
            <!--@Html.ActionLink("Detalles", "Details", new { id=item.globalServiceId })-->
            <a href="@Url.Action("Details", "GlobalServices", new { id = item.globalServiceId })" id="prueba"><img src="@Url.Content("~/Content/images/detail.png")" title="Detalles" /> </a>  |
            <!--@Html.ActionLink("Eliminar", "Delete", new { id=item.globalServiceId })-->
            <a href="@Url.Action("Delete", "GlobalServices", new { id = item.globalServiceId })" id="prueba"><img src="@Url.Content("~/Content/images/trash.png")" title="Eliminar" /> </a> 
        </td>
    </tr>
}

</table>
